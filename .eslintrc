{
    "extends": "airbnb/base",
    "rules": {
        "no-underscore-dangle": "off",
        "no-plusplus": "off",
        "import/extensions": "off",
        "no-unused-vars": "warn",
        "linebreak-style": [
            "error",
            "windows"
        ],
        "indent": ["error", 4],
        "no-console": "off",
        "eol-last": "off",
        "no-multiple-empty-lines": "off"
    }
}